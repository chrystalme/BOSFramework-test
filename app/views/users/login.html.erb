<h1>User#login</h1>
<p>Find me in app/views/users/login.html.erb</p>
<h2>
  <%= form_with(model: @user, url: user_login_path) do |form| %>
  <%= form.label "Username " %>
  <%= form.text_field :username, placeholder: "Username" %>
  <%= form.label "Password " %>
  <%= form.password_field :password, placeholder: "Password" %>
  <%= form.submit "Login"  %>
<% end %>
</h2>
